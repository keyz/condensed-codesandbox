{
  "name": "@condensed/codesandbox",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "consistent-versions": "syncpack list-mismatches",
    "prettier:check": "prettier --check \"**/*.{css,html,js,json,md,mdx,svg,ts,tsx,yaml,yml}\" --ignore-path ./.prettierignore",
    "prettier:write": "prettier --write \"**/*.{css,html,js,json,md,mdx,svg,ts,tsx,yaml,yml}\" --ignore-path ./.prettierignore"
  },
  "devDependencies": {
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "syncpack": "^12.3.0"
  },
  "workspaces": [
    "react/*"
  ],
  "packageManager": "npm@10.5.0"
}
